{
  "name": "template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-storybook": "REACT_APP_LOAD_STORYBOOK=storybook react-native run-android",
    "android-build:release": "cd android && rm -rf /android/app/build/outputs/apk/release && ./gradlew clean assembleRelease && cd ../",
    "android-build:debug": "cd android  && rm -rf /android/app/build/outputs/apk/debug && ./gradlew clean assembleDebug && cd ../",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier  --check \"(__tests__||src)/**/*.{ts(x),js}\"",
    "prettier:fix": "prettier --check --write \"(__tests__||src)/**/*.{ts(x),js}\"",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.6",
